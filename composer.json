{
    "name": "atanvarno/cache",
    "description": "PSR-16 cache implementation",
    "keywords": [
         "PSR-16", "cache"
    ],
    "homepage": "https://github.com/atanvarno69/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/simple-cache": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8"
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Cache\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
